<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.userController">
   <children>
      <AnchorPane prefHeight="428.0" prefWidth="475.0">
         <children>
            <Button layoutX="358.0" layoutY="16.0" mnemonicParsing="false" onAction="#changeToMenu" text="Return to Menu" />
            <ListView fx:id="cocktailRecipeList" layoutX="23.0" layoutY="67.0" prefHeight="341.0" prefWidth="385.0" />
            <Button fx:id="viewButton" layoutX="189.0" layoutY="12.0" mnemonicParsing="false" onAction="#viewRecipe" prefHeight="33.0" prefWidth="52.0" text="View" />
            <Button fx:id="deleteButton" layoutX="255.0" layoutY="12.0" mnemonicParsing="false" onAction="#deleteRecipe" prefHeight="33.0" prefWidth="52.0" text="Delete" />
            <TextField fx:id="searchName" layoutX="23.0" layoutY="16.0" promptText="Search:" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
